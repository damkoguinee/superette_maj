                <td>{{ retour_product.dateRetour ? retour_product.dateRetour|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateSaisie</th>
                <td>{{ retour_product.dateSaisie ? retour_product.dateSaisie|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_logescom_vente_retour_product_index') }}">back to list</a>

    <a href="{{ path('app_logescom_vente_retour_product_edit', {'id': retour_product.id}) }}">edit</a>

