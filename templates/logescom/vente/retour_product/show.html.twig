{% extends 'base.html.twig' %}

{% block title %}RetourProduct{% endblock %}

{% block body %}
    <h1>RetourProduct</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ retour_product.id }}</td>
            </tr>
            <tr>
                <th>Quantite</th>
                <td>{{ retour_product.quantite }}</td>
            </tr>
            <tr>
                <th>PrixVente</th>
                <td>{{ retour_product.prixVente }}</td>
            </tr>
            <tr>
